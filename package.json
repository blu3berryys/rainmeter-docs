{
  "name": "rainmeter-docs",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "5.4.2"
  },
  "scripts": {
    "start": "hexo server --log",
    "generate": "hexo generate",
    "build": "npm run clean && npm run build:hexo && npm run build:css",
    "build:hexo": "hexo generate",
    "build:css": "postcss ./public/css/*.css --replace --verbose",
    "clean": "hexo clean"
  },
  "dependencies": {
    "bootstrap": "5.3.3",
    "ejs": "3.1.10",
    "hexo": "5.4.2",
    "hexo-renderer-ejs": "2.0.0",
    "hexo-server": "3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "10.4.20",
    "browserslist": "4.23.3",
    "cssnano": "7.0.6",
    "postcss": "8.4.49",
    "postcss-cli": "11.0.0"
  }
}
